{
  "expo": {
    "name": "storylines",
    "slug": "storylines",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sl_icon_1024.png",
    "scheme": "storylines",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/sl_logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "imageWidth": 650
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imisshtml.storylines",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sl_icon_1024.png",
        "backgroundColor": "#050908"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.imisshtml.storylines"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sl_logo.jpg",
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "imageWidth": 650,
          "dark": {
            "image": "./assets/images/sl_logo.jpg",
            "resizeMode": "contain",
            "backgroundColor": "#000000",
            "imageWidth": 650
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4846990712111779~8324709931",
          "iosAppId": "ca-app-pub-4846990712111779~3524358138"
        }
      ],
      [
        "expo-updates",
        {
          "username": "imisshtml"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "minSdkVersion": 24,
            "ndkVersion": "25.1.8937393"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-notifications",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "86831c86-e42b-4466-8f77-ec933f0d1cef"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/86831c86-e42b-4466-8f77-ec933f0d1cef"
    }
  }
}
